    {
      "parameters": {
        "jsCode": "// Validate and structure patient task data\nconst payload = $input.item.json.body;\n\n// Validation\nif (!payload.patientId || !payload.taskType) {\n  throw new Error('Missing required fields: patientId and taskType');\n}\n\n// Structure the task\nconst task = {\n  taskId: `TASK-${Date.now()}`,\n  patientId: payload.patientId,\n  taskType: payload.taskType,\n  priority: payload.priority || 'medium',\n  description: payload.description || '',\n  dueDate: payload.dueDate || new Date(Date.now() + 86400000).toISOString().split('T')[0],\n  assignedTo: payload.assignedTo || 'Unassigned',\n  status: 'pending',\n  createdAt: new Date().toISOString(),\n  createdBy: 'MCP-Server',\n  metadata: payload.metadata || {}\n};\n\n// Audit log\nconst auditLog = {\n  timestamp: new Date().toISOString(),\n  action: 'create_patient_task',\n  patientId: task.patientId,\n  taskId: task.taskId,\n  user: 'system',\n  ipAddress: $input.item.json.headers['x-forwarded-for'] || 'internal'\n};\n\nreturn {\n  task,\n  auditLog\n};"
      },
      "id": "validate-structure",
      "name": "Validate & Structure Task",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [450, 300]
    },
    {
      "parameters": {
        "conditions": {
          "string": [
            {
              "value1": "={{ $json.task.priority }}",
              "operation": "equals",
              "value2": "urgent"
            },
            {
              "value1": "={{ $json.task.priority }}",
              "operation": "equals",
              "value2": "high"
            }
          ]
        },
        "combineOperation": "any"
      },
      "id": "check-priority",
